<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel="stylesheet" href="/stylesheets/style.css" />
  </head>
  <body>
    <h1>Messages</h1>
    <ul>
      <% messages.forEach(function(message) {%>
      <li>
        <p><%= message.text%></p>
        <p>Posted by: <%= message.user%></p>
        <p><%= message.added%></p>
      </li>
      <% }) %>
    </ul>
    <a href="/new">New Message</a>
  </body>
</html>
